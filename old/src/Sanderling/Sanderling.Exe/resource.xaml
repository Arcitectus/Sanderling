<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib"
    xmlns:BotEngine.UI.ViewModel="clr-namespace:BotEngine.UI.ViewModel;assembly=BotEngine.UI">
	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="pack://application:,,,/Sanderling.UI;component/resource.xaml"></ResourceDictionary>

		<ResourceDictionary>
			<system:String x:Key="AppVersionId">2019-05-13</system:String>

			<BotEngine.UI.ViewModel:AppProperty
                x:Key="AppProperty">
				<BotEngine.UI.ViewModel:AppProperty.CompileTime>
					<BotEngine.UI.ViewModel:AppPropertyCompileTime
                        WebsiteUrl="https://github.com/Arcitectus/Sanderling"
                        VersionId="{StaticResource AppVersionId}"/>
				</BotEngine.UI.ViewModel:AppProperty.CompileTime>

				<BotEngine.UI.ViewModel:AppProperty.RunTime>
					<BotEngine.UI.ViewModel:AppPropertyRunTimeConstructFromCurrentProcess />
				</BotEngine.UI.ViewModel:AppProperty.RunTime>

				<BotEngine.UI.ViewModel:AppProperty.View>
					<BotEngine.UI.ViewModel:AppPropertyView ArgumentsVisible="True" />
				</BotEngine.UI.ViewModel:AppProperty.View>
			</BotEngine.UI.ViewModel:AppProperty>

		</ResourceDictionary>
	</ResourceDictionary.MergedDictionaries>
</ResourceDictionary>